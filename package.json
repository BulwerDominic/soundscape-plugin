{
  "name": "magenta-studio",
  "version": "1.0.0",
  "description": "Magenta Studio plugins as Standalone applications and for Ableton Live",
  "main": "wrong.js",
  "scripts": {
    "watch": "concurrently \"npm run watch:plugin --dir $npm_config_dir\" \"npm run electron:plugin $npm_config_dir\"",
    "postinstall": "npm run download:models; cd magenta4live.amxd; npm install",
    "watch:standalone": "concurrently \"webpack -w --mode=development --env standalone --dir $npm_config_dir\" \"npm run electron:plugin $npm_config_dir\"",
    "watch:plugin": "webpack -w --mode=development --dir $npm_config_dir",
    "electron:plugin": "cd $npm_config_dir && electron ./build/Main.js",
    "webpack:build": "webpack --mode=production --env production && npm run copy",
    "webpack:standalone": "webpack --mode=production --env production --env standalone && npm run copy",
    "copy": "node components/copy.js",
    "build": "node electron/build.js --output $npm_config_output",
    "test": "node test/Test.js",
    "download:models": "node ./scripts/download_models"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/magenta-studio"
  },
  "author": "Magenta Team (https://g.co/magenta)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/helper-compilation-targets": "^7.16.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.16.0",
    "@polymer/lit-element": "^0.6.2",
    "@polymer/polymer": "^3.4.1",
    "autoprefixer": "^10.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.2.3",
    "chroma-js": "^2.1.2",
    "colors": "^1.4.0",
    "concurrently": "^6.3.0",
    "core-js": "^3.19.0",
    "css-loader": "^6.5.0",
    "drag-drop": "^7.2.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-packager": "^15.4.0",
    "eslint": "^8.1.0",
    "exports-loader": "^3.1.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.2",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^3.1.1",
    "lit-html": "^2.0.1",
    "node-sass": "^6.0.1",
    "node-zip": "^1.1.1",
    "postcss-loader": "^6.2.0",
    "request": "^2.88.0",
    "sass-loader": "^12.3.0",
    "spectron": "^15.0.0",
    "string-to-dom": "^0.1.5",
    "style-loader": "^3.3.1",
    "throttle-debounce": "^3.0.1",
    "tmp": "0.2.1",
    "tone": "^14.7.77",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "whatwg-fetch": "^3.6.2",
    "yargs": "^17.2.1"
  },
  "dependencies": {
    "@magenta/music": "^1.23.1",
    "tmp-promise": "^3.0.3"
  }
}
